{
   "manifest_version": 2,
   "name": "Czo Auto Complete",
   "version": "1.3",

   "description": "Set 'autocomplete=on' on password fields, so they will be remembered.",

   "icons": {
     "32": "icons/32.png",
     "48": "icons/48.png",
     "96": "icons/96.png",
     "128": "icons/128.png"
   },
 
   "content_scripts": [ {
      "js": [ "autocomplete.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ]

}
